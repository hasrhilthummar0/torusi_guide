      cursor: pointer;
      transition: background 0.25s, transform 0.2s ease-in-out;
    }

    .close-btn:hover {
      background: #ff4c4c;
      color: #fff;
      transform: scale(1.1);
    }

    /* Entry animation */
    @keyframes popupBounce {
      0% {
        opacity: 0;
        transform: translate(-50%, -50%) scale(0.8);
      }

      60% {
        opacity: 1;
        transform: translate(-50%, -45%) scale(1.05);
      }

      100% {
        transform: translate(-50%, 0%) scale(1);
      }
    }

