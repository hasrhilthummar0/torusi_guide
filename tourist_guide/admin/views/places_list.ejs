<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="/css/bootstrap.min.css">
</head>
<body>
<div class="container mt-5">
  <h2><%= title %></h2>
  <a href="/admin/places/add" class="btn btn-primary mb-3">+ Add New Place</a>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>ID</th><th>Title</th><th>Slogan</th><th>Country</th><th>Image</th>
      </tr>
    </thead>
    <tbody>
      <% places.forEach(place => { %>
        <tr>
          <td><%= place.id %></td>
          <td><%= place.title %></td>
          <td><%= place.slogan %></td>
          <td><%= place.country %></td>
          <td><img src="<%= place.image %>" width="100"></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>
</body>
</html>
